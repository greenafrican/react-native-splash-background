{
    "compilerOptions": {
      "target": "ES2019",
      "module": "CommonJS",
      "lib": ["ESNext"],
      "strict": true,
      "jsx": "react",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "skipLibCheck": true,
      "declaration": true,
      "declarationMap": true,
      "outDir": "lib/typescript",
      "rootDir": "."
    },
    "exclude": ["node_modules", "lib"]
  }
  